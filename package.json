  "license": "MIT",
  "dependencies": {
    "babel-preset-react": "^6.5.0",
    "google-auth-library": "^0.9.5",
    "googleapis": "^2.0.4",
    "immutable": "^3.7.3",
    "koa": "^1.2.0",
    "koa-accesslog": "0.0.2",
    "koa-bodyparser": "^2.0.0",
    "koa-mount": "^1.3.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^5.1.2",
    "koa-session": "^3.1.0",
    "koa-static": "^2.0.0",
    "moment": "^2.10.3",
    "react": "^15.0.0-rc.2",
    "react-dom": "^15.0.0-rc.2",
    "react-redux": "^4.4.1",
